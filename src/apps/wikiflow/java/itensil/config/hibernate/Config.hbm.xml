<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="itensil.config.hibernate">

	<class name="ConfigEntity" lazy="false" table="iten_config">
		<id name="component">
            <generator class="assigned"/>
        </id>
        <property name="version" column="vNumber"/>
        <property name="properties"/>
	</class>
	
</hibernate-mapping>
